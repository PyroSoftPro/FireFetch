{
  "name": "firefetch",
  "version": "25.6.12",
  "description": "A modern video downloader with a sleek dark interface",
  "main": "app.js",
  "scripts": {
    "start": "npx electron .",
    "server": "echo 'Server is now integrated with Electron. Use npm start instead.'",
    "dev": "npm start",
    "build-win": "electron-builder --win",
    "build-mac": "electron-builder --mac",
    "build-linux": "electron-builder --linux",
    "build-clean": "electron-builder build --win --dir --config.asar=false",
    "build-portable": "electron-builder --win portable",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "adm-zip": "^0.5.10",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "path": "^0.12.7",
    "peerflix": "^0.39.0"
  },
  "devDependencies": {
    "electron": "^27.3.11",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2"
  },
  "build": {
    "appId": "com.firefetch.app",
    "productName": "FireFetch",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        "dir",
        "portable"
      ],
      "icon": "icon.ico"
    },
    "portable": {
      "artifactName": "FireFetch-Portable.exe",
      "unpackDirName": "FireFetchData"
    },
    "files": [
      "**/*",
      "!*.log",
      "!dist",
      "!build-*.bat",
      "!downloads/**/*",
      "!cookies/**/*",
      "!node_modules/*/.git/**",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "asarUnpack": [
      "dep/**/*"
    ],
    "extraResources": [
      {
        "from": "dep",
        "to": "dep"
      },
      {
        "from": "public",
        "to": "public"
      }
    ]
  }
}
